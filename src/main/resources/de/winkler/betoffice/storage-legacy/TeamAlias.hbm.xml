<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="de.winkler.betoffice.storage.TeamAlias"
        table="bo_teamalias"
        dynamic-update="false"
        dynamic-insert="false"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="native">
            </generator>
        </id>

        <property
            name="aliasName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="bo_aliasname"
            not-null="true"
            unique="true"
        />

        <many-to-one
            name="team"
            class="de.winkler.betoffice.storage.Team"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="bo_team_ref"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Team.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
