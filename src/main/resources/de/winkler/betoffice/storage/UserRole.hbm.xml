<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="de.winkler.betoffice.storage.UserRole" table="bo_userrole"
    dynamic-update="false" dynamic-insert="false">

    <id name="id" column="id" type="java.lang.Long">
      <generator class="native" />
    </id>

    <many-to-one name="user" class="de.winkler.betoffice.storage.User"
      cascade="none" outer-join="auto" update="true" insert="true" column="bo_user_ref" />

    <property name="" column="bo_status" update="true" insert="true">
      <type name="org.hibernate.type.EnumType">
        <param name="enumClass">de.winkler.betoffice.storage.enums.TippStatusType</param>
      </type>
    </property>
  </class>

</hibernate-mapping>